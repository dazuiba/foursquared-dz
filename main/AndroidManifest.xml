      android:label="@string/app_name" >
      <intent-filter>
        <action
          android:name="com.joelapenna.foursquared.intent.action.LOGGED_OUT" />
      </intent-filter>
    </activity>
    
    <activity
      android:name=".TipActivity"
      android:label="@string/app_name" >
      <intent-filter>
        <action
          android:name="com.joelapenna.foursquared.intent.action.LOGGED_OUT" />
      </intent-filter>
